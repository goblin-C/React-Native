    "web": "expo start --web"
  },
  "dependencies": {
    "@react-native-community/cli": "^20.0.2",
    "expo": "~54.0.12",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-config": "^1.5.5"
  },
  "private": true
}
