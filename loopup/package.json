{
  "name": "loopup",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android -d",
    "dev": "npx react-native run-android --mode devDebug --appId com.loopup.dev",
    "qa": "npx react-native run-android --mode qaDebug --appId com.loopup.qa",
